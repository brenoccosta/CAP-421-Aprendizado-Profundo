    "\n",
    "imgs_dir = \"./Data/Images\"\n",
    "walkthrough = os.walk(imgs_dir)\n",
    "\n",
    "for root, folders, files in walkthrough:\n",
    "  if files:\n",
    "    for file in files:\n",
    "      remove = \"NDVI\" in file\n",
    "      if remove: os.remove(os.path.join(root, file))"
